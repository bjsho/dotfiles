{
    "profiles": [
        {
            "complex_modifications": {
                "rules": [
                    {
                        "manipulators": [
                            {
                                "description": "Make Caps Lock the Hyper Key and make the run various applications",
                                "from": {
                                    "key_code": "caps_lock",
                                    "modifiers": { "optional": ["any"] }
                                },
                                "to": [
                                    {
                                        "key_code": "left_shift",
                                        "modifiers": ["left_command", "left_control", "left_option"]
                                    }
                                ],
                                "type": "basic"
                            },
                            {
                                "description": "Open MS Word",
                                "from": {
                                    "key_code": "w",
                                    "modifiers": { "mandatory": ["left_control", "left_option", "left_command", "left_shift"] }
                                },
                                "to": [{ "shell_command": "open -a 'Microsoft Word.app'" }],
                                "type": "basic"
                            },
                            {
